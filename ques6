programa
{
	
inteiro n1, n2, n3, n4
	funcao inicio()
	{
		escreva("me fale 4 numbers ")
		leia(n1, n2, n3, n4)

		se(n1 > n2 e n1 > n3 e n1 > n4){
			escreva("o maior numero é ", n1)
		}
			se(n2 > n1 e n2 > n3 e n2 > n4){
				escreva("o maior numero é ", n2)
			}
			se(n3 > n1 e n3 > n2 e n3 > n4){
				escreva("o maior numero é ", n3)
		}senao{
			escreva("o maior numero é ", n4)
		}
		
	}
}
